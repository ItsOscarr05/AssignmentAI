{
  "name": "assignmentai",
  "version": "1.0.0",
  "description": "A comprehensive assignment management system with AI-powered features",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "test": "bash scripts/run-coverage.sh",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "test:watch": "concurrently \"npm run test:frontend:watch\" \"npm run test:backend:watch\"",
    "test:frontend:watch": "cd frontend && npm test -- --watch",
    "test:backend:watch": "cd backend && npm test -- --watch",
    "lint": "concurrently \"npm run lint:frontend\" \"npm run lint:backend\"",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "lint:fix": "concurrently \"npm run lint:frontend:fix\" \"npm run lint:backend:fix\"",
    "lint:frontend:fix": "cd frontend && npm run lint:fix",
    "lint:backend:fix": "cd backend && npm run lint:fix",
    "build": "concurrently \"npm run build:frontend\" \"npm run build:backend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "analyze": "cd frontend && npm run analyze",
    "performance": "cd frontend && npm run performance",
    "prepare": "husky install"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@tanstack/react-query": "^5.80.7",
    "axios": "^1.10.0",
    "concurrently": "^8.2.0",
    "react-router-dom": "^7.6.2",
    "sonner": "^2.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.4",
    "husky": "^8.0.3",
    "jest-junit": "^16.0.0",
    "jest-react-native": "^18.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^13.2.3",
    "vitest": "^3.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "author": "AssignmentAI Team",
  "license": "MIT"
}
